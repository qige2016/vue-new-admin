{
  "name": "vue-new-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "element-theme": "et",
    "serve": "element-theme && vue-cli-service serve",
    "build": "element-theme && vue-cli-service build",
    "start": "npm run serve",
    "report": "element-theme && vue-cli-service build --report",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "new": "plop"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:unit"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "0.19.0",
    "core-js": "3.3.2",
    "dayjs": "1.8.15",
    "element-ui": "2.12.0",
    "file-saver": "2.0.2",
    "normalize.css": "8.0.1",
    "screenfull": "4.2.0",
    "sha1": "1.1.1",
    "vue": "2.6.10",
    "vue-router": "3.0.3",
    "vuex": "3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.0.0",
    "@vue/cli-plugin-eslint": "4.0.0",
    "@vue/cli-plugin-unit-jest": "4.0.0",
    "@vue/cli-service": "4.0.0",
    "@vue/eslint-config-prettier": "5.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "10.0.3",
    "babel-plugin-component": "1.1.1",
    "compression-webpack-plugin": "3.0.0",
    "element-theme-chalk": "2.13.0",
    "eslint": "5.16.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-vue": "5.0.0",
    "husky": "3.0.0",
    "lint-staged": "9.2.0",
    "node-sass": "4.13.0",
    "plop": "2.5.3",
    "prettier": "1.18.2",
    "sass-loader": "8.0.0",
    "script-ext-html-webpack-plugin": "2.1.3",
    "svg-sprite-loader": "4.1.6",
    "svgo": "1.2.2",
    "vue-cli-plugin-element": "1.0.1",
    "vue-template-compiler": "2.6.10"
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
  }
}
